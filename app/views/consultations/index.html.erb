<%= month_calendar(events: @events, attribute: :at) do |date, events| %>
  <%= date %>

  <% events.each do |event| %>
      <% if event.class.name == "Consultation" %>
        <%= link_to "", timeline_path(focus: "consultation_#{event.id}"), class: "calendar-consultation" %>
      <% else %>
        <%= link_to "", timeline_path(focus: "symptom_#{event.id}"), class: "calendar-symptom" %>
      <% end %>
  <% end %>
<% end %>